{
    "input": {
        "default_mode": "voice",
        "fallback_mode": "text"
    },
    "output": {
        "voice_enabled": true,
        "gui_enabled": false
    },
    "ai": {
        "model": "gpt-3.5-turbo",
        "temperature": 0.7,
        "azure": {
            "endpoint": "https://models.inference.ai.azure.com",
            "models": {
                "vision": "gpt-4o",
                "llama": "Llama-3.3-70B-Instruct",
                "cohere": "Cohere-command-r-plus"
            }
        }
    },
    "plugins": {
        "enabled": true,
        "auto_load": true
    },
    "logging": {
        "level": "INFO",
        "file_logging": true
    }
}
